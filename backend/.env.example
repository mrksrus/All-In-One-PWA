# Server Configuration
PORT=3000
NODE_ENV=production

# JWT Secrets (generate random strings - use: openssl rand -hex 32)
JWT_SECRET=change-this-to-a-random-secret-key-minimum-32-characters
JWT_REFRESH_SECRET=change-this-to-another-random-secret-key-minimum-32-characters

# Database
DB_PATH=/data/database.sqlite

# CORS - Set to your domain (or http://localhost:5173 for development)
CORS_ORIGIN=http://localhost:5173
